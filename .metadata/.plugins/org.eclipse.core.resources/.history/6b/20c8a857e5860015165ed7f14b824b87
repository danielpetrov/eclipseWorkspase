<?php
/* Задача 25:
Да се направи програма, която по дадено число N,да изчислява
N!,т.е. 1*2*3*4...*N.
Пример: 5
Изход: 120
Използвайте цикъл do-while. */
require_once 'Readline.php';

$endingIndex = ( int ) readline ( "Enter number:" . PHP_EOL );

$currentNumber = 1;

$sumOfAllPowers = 0;

do {
	$sumOfAllPowers *= $currentNumber++;
	
}while($currentNumber <= $endingIndex);

echo $sumOfAllPowers;